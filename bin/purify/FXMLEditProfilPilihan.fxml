<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefWidth="300.0" spacing="15.0" style="-fx-background-color: #E8F5E9;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="purify.FXMLEditProfilPilihanController">
   <children>
      <Label styleClass="sub-header" text="Apa yang ingin Anda ubah?">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Button fx:id="btnUbahUsername" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#handleUbahUsername" stylesheets="@purify-theme.css" text="Ubah Username" />
      <Button fx:id="btnUbahSandi" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#handleUbahSandi" stylesheets="@purify-theme.css" text="Ubah Sandi" />
      <Button fx:id="btnBatal" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#handleBatal" styleClass="secondary-button" stylesheets="@purify-theme.css" text="Batal" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>